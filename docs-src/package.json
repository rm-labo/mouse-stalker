{
  "name": "docs",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "link": "cd ../ && npm run build && npm link && cd ./docs-src && npm link @rm-labo/mouse-stalker",
    "start": "docker-compose down && docker-compose up -d",
    "stop": "docker-compose down",
    "sync": "browser-sync start --config bs-config.js",
    "build": "npm run link && NODE_ENV=production npm run lintfix && webpack --mode production",
    "dev": "npm run link && NODE_ENV=development webpack --watch --mode development & npm run sync",
    "clean": "rimraf ../docs/**/assets/**/*.map",
    "lint": "npm run lint:ts & npm run lint:css",
    "lintfix": "npm run lintfix:ts & npm run lintfix:css",
    "lint:ts": "eslint --ignore-path .eslintignore .",
    "lintfix:ts": "eslint --fix --ignore-path .eslintignore .",
    "lint:css": "stylelint 'src/scss/**/*.scss'",
    "lintfix:css": "stylelint 'src/scss/**/*.scss' --fix",
    "precommit": "npm run build"
  },
  "author": "rm-labo",
  "license": "ISC",
